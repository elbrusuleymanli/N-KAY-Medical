<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewData["Title"] - NKAYM</title>
    <!-- CSS only -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="https://unpkg.com/aos@2.3.0/dist/aos.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css" />
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.15.4/css/all.css"
          integrity="sha384-DyZ88mC6Up2uqS4h/KRgHuoeGwBcD4Ng9SiP4dIRy0EXTlnuz47vAwmeGwVChigm" crossorigin="anonymous">
    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.carousel.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.7.0/animate.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.theme.default.min.css">


    <link rel="stylesheet" href="~/assets/css/style.css">
    <title>NKayM</title>
    @RenderSection("sliderBg", false)
</head>

<body>
    <header>
        <section id="navigation-bar">
            <div class="navigation">
                <div class="container">
                    <div class="row">
                        <div class="col-md-1 ">
                            <div class="medical-heart">
                                <i class="fas fa-heartbeat"></i>
                            </div>
                        </div>
                        <div class="col-md-2">
                            <div class="sosial-media">
                                <ul>
                                    <li>
                                        <a href="https://www.facebook.com/NKAYMclinics/" target="_blank">
                                            <i class="fab fa-facebook"></i>
                                        </a>
                                    </li>
                                    <li><a href=""><img src="~/assets/image/Icons/instagram.png" style="width:35px" alt=""></a></li>
                                    <li><a href=""><i class="fab fa-whatsapp"></i></a></li>
                                    <li>
                                        <a href="https://www.youtube.com/watch?v=LyV7Vl7PUPU" target="_blank">
                                            <i class="fab fa-youtube"></i>
                                        </a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="topic">
                                <p>"İnsanların çoxunun təqdir etmədiyi iki şey var: sağlamlıq və boş vaxt."</p>
                                <p class="author">Məhəmməd Peyğəmbər (s.a.s)</p>
                            </div>
                        </div>
                        <div class="col-md-2">
                            <div class="randevu">

                                <div class="main-side">
                                    <a href="">HƏKİM QƏBULU</a>
                                </div>
                                <div class="letter">
                                    <a href=""> <i class="far fa-envelope"></i></a>

                                </div>
                            </div>
                        </div>
                        <div class="col-md-1">
                            <div class="language">
                                <ul>
                                    <li>
                                        <a href="">
                                            <img src="~/assets/image/azerbaijan_sphere_icon_64.png" alt="">
                                        </a>
                                    </li>

                                    <li>
                                        <a href="">
                                            <img src="~/assets/image/russia_sphere_icon_64.png" alt="">
                                        </a>
                                    </li>

                                </ul>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </section>

        <section id="navigation-menu" class="header-main">
            <div class="container">
                <div class="row">
                    <div class="col-md-1">
                        <div class="logo">
                            <a asp-controller="Home" asp-action="Index">
                                <img src="~/assets/image/logo (1).png" alt="">
                            </a>

                        </div>
                    </div>
                    <div class="col-md-9">
                        <div class="menu-bar">
                            <ul>
                                <li class="drop-down "><a asp-controller="Home" asp-action="Index">ANA SƏHİFƏ</a></li>
                                <li class="drop-down ">
                                    <a asp-controller="About" asp-action="Index">HAQQIMIZDA <i class="fas fa-syringe"></i></a>
                                    <ul class="drop-down-content">
                                        <i class="fas fa-caret-up"></i>
                                        <li><a asp-controller="About" asp-action="Index">TARİXİMİZ</a></li>
                                        <li><a asp-controller="Manager" asp-action="Index">RƏHBƏRLİK</a></li>
                                    </ul>
                                </li>
                                <li class="drop-down "><a asp-controller="Service" asp-action="Index">XİDMƏTLƏR</a></li>
                                <li class="drop-down "><a asp-controller="Doctor" asp-action="Index">HƏKİMLƏR</a> </li>
                                <li class="drop-down "><a asp-controller="News" asp-action="Index">XƏBƏRLƏR</a></li>
                                <li class="drop-down ">
                                    <a href="">QALEREYA <i class="fas fa-syringe"></i></a>
                                    <ul class="drop-down-content">
                                        <i class="fas fa-caret-up"></i>
                                        <li><a href="">ŞƏKİLLƏR</a></li>
                                        <li><a href="">VİDEOLAR</a></li>
                                    </ul>
                                </li>
                                <li class="drop-down "><a asp-controller="Contact" asp-action="Index">ƏLAQƏ</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-1">
                        <div class="profile">
                            <button id="myBtn" type="submit">
                                <i class="fas fa-user-md"></i>
                            </button>
                            <span class="sign-in">Daxil ol</span>
                        </div>
                    </div>

                    <div class="col-md-1">
                        <div class="box">
                            <div class="clock">
                                <span class="hours"></span>
                                <span class="minutes"></span>
                                <p class="brand">N-KAY M</p>

                            </div>

                        </div>

                    </div>

                </div>
            </div>
        </section>

        <!-- mobile menu start-->
        <div id="nav-toggle">
            <div>
                <span></span>
                <span></span>
                <span></span>
            </div>
        </div>

        <div id="gloval-nav">
            <nav>
                <ul id="navigation">
                    <li><a href="#">Ana Səhifə</a></li>
                    <li><a href="#">Haqqımızda</a> </li>
                    <li><a href="#">Xidmətlər</a></li>
                    <li><a href="#">Həkimlər</a></li>
                    <li><a href="#">Xəbərlər</a></li>
                    <li><a href="#">Qalereya</a></li>
                    <li><a href="#">Əlaqə</a></li>
                </ul>
            </nav>

            <div id="nav-bg-wrap">
                <div class="nav-bg home"></div>
                <div class="nav-bg about"></div>
                <div class="nav-bg service"></div>
                <div class="nav-bg doctors"></div>
                <div class="nav-bg news"></div>
                <div class="nav-bg gallery"></div>
                <div class="nav-bg contact"></div>
            </div>
        </div>


        <!-- mobile menu end-->
    </header>
    @RenderBody()
    <footer>
        <!-- footer start-->
        <div class="footer">
            <div class="container">
                <div class="row">
                    <div class="col-lg-3">

                        <div class="logo">
                            <a asp-controller="Home" asp-action="Index">
                                <img src="~/assets/image/logo (1).png" alt="">
                            </a>
                        </div>
                    </div>
                    <div class="col-lg-3">
                        <div class="about-us">
                            <h4>HAQQIMIZDA</h4>
                            <p>
                                Klinikanın təməli təsisçiləri həkim-stomatoloqlar Natiq Mütəllimov və Kəbutər Mütəllimova tərəfindən
                                1996-cı
                                ildə qoyulmuşdur və 2005-ci il iyulun 26-da istifadəyə verilmişdir.
                            </p>
                            <a asp-controller="About" asp-action="Index">
                                ərtaflı
                                <i class="fas fa-angle-double-right"></i>
                            </a>
                        </div>
                    </div>
                    <div class="col-lg-3">
                        <h4>KATEQORİYA</h4>
                        <ul>
                            <li><a asp-controller="Service" asp-action="Index">Xidmətlər</a></li>
                            <li><a asp-controller="Doctor" asp-action="Index">Həkimlər</a></li>
                            <li><a asp-controller="News" asp-action="Index">Xəbərlər</a></li>
                            <li><a href="">Qalereya</a></li>
                            <li><a asp-controller="Vacancy" asp-action="Index">Vakansiya</a></li>
                        </ul>
                    </div>
                    <div class="col-lg-3">
                        <div class="contacts">
                            <h4>ƏLAQƏ</h4>
                            <ul class="contact">
                                <li class="adress"> <span>Ünvan:</span> 49A,N.Nərimanov,Xaçmaz,2700 </li>
                                <li class="phone"><span>Telefon:</span>  (+9942332) 5 18 54</li>
                                <li class="email">
                                    <span>Email:</span><a href="mailto:nkay@medical.com"> nkay@medical.com</a>
                                </li>
                            </ul>

                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- footer end-->
        <!-- line start-->
        <div class="end-line">
            <span>N-KAY MEDICAL © 2022 Bütün hüquqlar qorunur. </span>
            <span>Designed by ENAPA</span>
        </div>
        <!-- line end-->
    </footer>
    <!-- back to top start-->

    <div class="btn-back-to-top">
        <i class="far fa-chevron-up first"></i>

    </div>

    <!-- back to top end-->
    <!-- modal open by Login -->
    <div id="myModal" class="modal">

        <!-- Modal content -->
        <div class="login-wrap">
            <div class="login-html">
                <input id="tab-1" type="radio" name="tab" class="sign-in" checked><label for="tab-1" class="tab">
                    DAXİL
                    OL
                </label>
                <input id="tab-2" type="radio" name="tab" class="sign-up"><label for="tab-2" class="tab">QEYDİYYAT</label>
                <div class="login-form">
                    <div class="sign-in-htm">
                        <div class="group">
                            <label for="user" class="label">İSTİFADƏÇİ ADI</label>
                            <input id="user" type="text" class="input">
                        </div>
                        <div class="group">
                            <label for="pass" class="label">ŞİFRƏ</label>
                            <input id="pass" type="password" class="input" data-type="password">

                        </div>
                        <div class="group">
                            <input id="check" type="checkbox" class="check" checked>
                            <label for="check"><span class="icon"></span> Yadda saxla</label>
                        </div>
                        <div class="group">
                            <input type="submit" class="button" value="Daxil ol">
                        </div>
                        <div class="hr"></div>
                        <div class="foot-lnk">
                            <a href="#forgot">Şifrəni unutdun?</a>
                        </div>
                    </div>
                    <div class="sign-up-htm">
                        <div class="group">
                            <label for="user" class="label">İSTİDAFƏÇİ ADI</label>
                            <input id="user" type="text" class="input">
                        </div>
                        <div class="group">
                            <label for="pass" class="label">ŞİFRƏ</label>
                            <input id="pass" type="password" class="input" data-type="password">
                        </div>
                        <div class="group">
                            <label for="pass" class="label">TƏKRAR ŞİFRƏ</label>
                            <input id="pass" type="password" class="input" data-type="password">
                        </div>
                        <div class="group">
                            <label for="pass" class="label">E-POÇT ÜNVANI</label>
                            <input id="pass" type="text" class="input">
                        </div>
                        <div class="group">
                            <input type="submit" class="button" value="Qeydiyyat">
                        </div>

                    </div>
                </div>
            </div>
        </div>

    </div>
    <!-- profile modal end-->
    <!-- modal open Appointment -->
    <!-- modal open end -->
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"
            integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.5/js/materialize.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/owl.carousel.min.js"></script>
    <script src="https://unpkg.com/aos@2.3.0/dist/aos.js"></script>
    <script src="https://npmcdn.com/isotope-layout@3/dist/isotope.pkgd.js"></script>
    <script>
        AOS.init();
    </script>
    <script src="~/assets/JS/main.js"></script>
    <script src="~/assets/JS/mobile-menu.js"></script>

</body>

</html>