@model AddServiceVM
@{
    ViewData["Title"] = "AddService";
}


<div class="main-panel">
    <div class="content-wrapper">
        <div class="row">
            <div class="col-lg-12 stretch-card">
                <div class="card">
                    <div class="card-body">

                        <h4>XİDMƏTLƏR ƏLAVƏ ET </h4>

                        <div class="table-responsive pt-3">

                            <form method="post" asp-controller="Service" asp-action="AddService">
                                <div class="form-group">

                                   
                                       <input type="text" />
                                  
                                    <span asp-validation-for="ServiceName" class="text-danger"></span>
                                </div>
                                <button type="submit" class="btn btn-primary">Təsdiqlə</button>
                                <button class="btn btn-danger">Bağla</button>
                            </form>




                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


