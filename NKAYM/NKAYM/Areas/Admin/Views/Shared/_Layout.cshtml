<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Material Dash</title>
    <!-- plugins:css -->
    <link rel="stylesheet" href="~/assets/Admin/vendors/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="~/assets/Admin/vendors/css/vendor.bundle.base.css">
    <!-- CSS only -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet" />
    <!-- endinject -->
    <!-- Plugin css for this page -->
    <!-- End plugin css for this page -->
    <!-- Layout styles -->
    <link rel="stylesheet" href="~/assets/Admin/css/demo/style.css">
    <!-- End layout styles -->
    <link rel="shortcut icon" href="~/assets/Admin/images/favicon.png" />
</head>
<body>
    <script src="~/assets/Admin/js/preloader.js"></script>
    <div class="body-wrapper">
        <!-- partial:../../partials/_sidebar.html -->
        <aside class="mdc-drawer mdc-drawer--dismissible mdc-drawer--open">
            <div class="mdc-drawer__header">
                <a asp-controller="Admin"  class="brand-logo">
                    <img src="~/assets/Admin/images/logo (1).png" alt="logo">
                </a>
            </div>
            <div class="mdc-drawer__content">

                <div class="mdc-list-group">
                    <nav class="mdc-list mdc-drawer-menu">
                        <div class="mdc-list-item mdc-drawer-item">
                            <a class="mdc-drawer-link" asp-controller="Home">
                                <i class="far fa-hospital material-icons mdc-list-item__start-detail mdc-drawer-item-icon" aria-hidden="true"
                                   style="margin-right:10px;font-size:20px"></i>
                                Admin
                            </a>
                        </div>
                        <div class="mdc-list-item mdc-drawer-item">
                            <a class="mdc-drawer-link" asp-controller="News" asp-action="Index">
                                <i class="fas fa-newspaper material-icons mdc-list-item__start-detail mdc-drawer-item-icon" aria-hidden="true"
                                   style="margin-right:10px;font-size:20px"></i>
                                Xəbərlər
                            </a>
                        </div>
                        <div class="mdc-list-item mdc-drawer-item">
                            <a class="mdc-expansion-panel-link" asp-controller="Service" asp-action="Index" data-target="ui-sub-menu">
                                <i class="material-icons mdc-list-item__start-detail mdc-drawer-item-icon" aria-hidden="true">dashboard</i>
                                Xidmətlər

                            </a>

                        </div>
                        <div class="mdc-list-item mdc-drawer-item">
                            <a class="mdc-drawer-link" asp-controller="Doctor" asp-action="Index">
                                <i class="fas fa-user-md material-icons mdc-list-item__start-detail mdc-drawer-item-icon" aria-hidden="true"
                                   style="margin-right:10px;font-size:20px"></i>
                                Həkimlər
                            </a>
                        </div>
                        <div class="mdc-list-item mdc-drawer-item">
                            <a class="mdc-drawer-link" asp-controller="Partner" asp-action="Index">
                                <i class="far fa-handshake material-icons mdc-list-item__start-detail mdc-drawer-item-icon" aria-hidden="true"
                                   style="margin-right:10px;font-size:20px"></i>
                                Tərəfdaşlar
                            </a>
                        </div>
                        <div class="mdc-list-item mdc-drawer-item">
                            <a class="mdc-expansion-panel-link" asp-controller="Photo" asp-action="Index">
                                <i class="far fa-images material-icons mdc-list-item__start-detail mdc-drawer-item-icon" aria-hidden="true"
                                   style="margin-right:10px;font-size:20px"></i>
                                Şəkillər

                            </a>

                        </div>
                        <div class="mdc-list-item mdc-drawer-item">
                            <a class="mdc-drawer-link" asp-controller="Video" asp-action="Index">
                                <i class="fas fa-film material-icons mdc-list-item__start-detail mdc-drawer-item-icon" aria-hidden="true"
                                   style="margin-right:10px;font-size:20px"></i>
                                Videolar
                            </a>
                        </div>
                        <div class="mdc-list-item mdc-drawer-item">
                            <a class="mdc-drawer-link" asp-controller="Vacancy" asp-action="Index">
                                <i class="far fa-address-book material-icons mdc-list-item__start-detail mdc-drawer-item-icon" aria-hidden="true"
                                   style="margin-right:10px;font-size:20px"></i>
                                Vakansiyalar
                            </a>
                        </div>
                        <div class="mdc-list-item mdc-drawer-item">
                            <a class="mdc-drawer-link" asp-controller="Ticker" asp-action="Index">
                                <i class="fas fa-bullhorn material-icons mdc-list-item__start-detail mdc-drawer-item-icon" aria-hidden="true" 
                                   style="margin-right:10px;font-size:20px"></i>
                                Məlumat
                            </a>
                        </div>
                    </nav>
                </div>


            </div>
        </aside>
        <!-- partial -->
        <div class="main-wrapper mdc-drawer-app-content">
            <!-- partial:../../partials/_navbar.html -->

            @RenderBody()

            <!-- partial -->
        </div>
    </div>
    <script src="https://kit.fontawesome.com/30032e1c2a.js" crossorigin="anonymous"></script>
    <!-- plugins:js -->
    <script src="~/assets/Admin/vendors/js/vendor.bundle.base.js"></script>
    <!-- endinject -->
    <!-- Plugin js for this page-->
    <!-- End plugin js for this page-->
    <!-- inject:js -->
    <script src="~/assets/Admin/js/material.js"></script>
    <script src="~/assets/Admin/js/misc.js"></script>
    <script src="~/assets/Admin/js/dashboard.js"></script>
    <!-- endinject -->
    <!-- Custom js for this page-->
    <!-- End custom js for this page-->
</body>
</html>
